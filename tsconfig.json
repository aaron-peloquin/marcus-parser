{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "allowJs": false,
      "jsx": "react",
      "outDir": "./build",
      "rootDir": "./lib",
      "removeComments": true,
      "noEmit": true,
      "pretty": true,
      "skipLibCheck": true,
      "strict": false,
      "moduleResolution": "node",
      "esModuleInterop": true
  }
}